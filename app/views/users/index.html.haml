.top
  %h2 Members
.main  
  - unless @users.empty?
    .inner_content
      - for user in @users
        .group_member_thumb
          = link_to image_tag(user.profile.avatar.url(:thumb)), user_path(user)
          %p
            = h user.name
            
.details
  .inner_content
    = render "shared/tall_advertisement"
