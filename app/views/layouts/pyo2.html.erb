<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
	<head>
		<meta content="text/html; charset=utf-8" http-equiv="Content-Type">
		<title>Put Yourself On</title>
		<link type="text/css" rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css">
		<%= stylesheet_link_tag 'style_v2' %>
		<!-- <script type="text/javascript" src="js/jquery-1.2.6.min.js" charset="uft-8"></script> -->
		<!-- <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.3.2/jquery.min.js" charset="uft-8"></script> -->
		<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js" type="text/javascript"></script>
	  	<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js" type="text/javascript"></script>
		<%= javascript_include_tag "nav", "elastic", "disabled" %>
	
		<script>
		
		$(document).ready(function() {
		$("#dash-tabs").tabs();
		});
		
		$(document).ready(function() {
		$("#inbox-tabs").tabs({
	    load: function(event, ui) {
	        $('a.internal-link', ui.panel).click(function() {
	            $(ui.panel).load(this.href);
	            return false;
	        });
	    }
	});
		});
		
		var $tabs = $('#inbox-tabs').tabs(); // first tab selected
		$('#not-text-link').click(function() { // bind click event to link
	    $tabs.tabs('select', 1); // switch to second tab
	    return false;
		});
		
		$(document).ready(function() {
		$("#inbox-msg-tabs").tabs();
		});
	
		</script>	
	</head>
	<body>

		<!-- WRAPPER -->
		<div id="wrapper">
			<%= render :partial=> 'layouts/top_bar_v2' %>
			<div id="pyo">
				<%= yield %>
			</div>
    		<div class="invisibleline"></div>
		</div>
    	<!-- /END WRAPPER -->

  </body>
</html>
