.video_thumb
  = link_to image_tag(video.thumb_url), user_video_path(video.user,video)
  %h3
    = link_to video.title, user_video_path(video.user,video)
  = link_to('delete', [video.user,video], :confirm =>"Are you sure you want to delete this photo?", :method => :delete)+"<br>" if is_owner?(video)